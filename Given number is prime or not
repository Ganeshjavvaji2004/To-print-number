#include<stdio.h>
void main()
{
    int n,i,count=0;
    printf("Enter the n value:");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
{
    count ++;
}
    }
    if(count==2)
    {
        printf("It is prime number");
    }
    else
    {
        printf("It is not a prome number");
    }
}
